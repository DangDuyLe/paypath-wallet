# PayPath Wallet Environment Variables
# Copy this file to .env (or .env.local) and fill in your values

# Backend API (used for auth + protected routes)
VITE_API_BASE_URL=http://localhost:3000

# zkLogin (Google)
VITE_ZKLOGIN_ENABLED=true
VITE_GOOGLE_CLIENT_ID= client-id

# Optional statement included in the wallet sign-in message
VITE_AUTH_STATEMENT=

# Gaian Payment Gateway API
VITE_GAIAN_API_URL=https://payments.gaian.io
VITE_GAIAN_API_KEY=your_api_key_here
